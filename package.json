{
  "name": "ucenter-backend",
  "version": "1.1.0",
  "description": "",
  "author": "",
  "private": true,
  "license": "UNLICENSED",
  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "build:prod": "nest build && nest start",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "NODE_ENV='production' node dist/main",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./ormconfig.js",
    "keygen": "openssl genrsa -out config/JWT_PRIVATE_KEY.pem 1024 && openssl rsa -in config/JWT_PRIVATE_KEY.pem -pubout > config/JWT_PUBLIC_KEY.pub",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@metaio/microservice-model": "^1.0.2",
    "@nestjs/axios": "^0.0.7",
    "@nestjs/common": "^8.4.1",
