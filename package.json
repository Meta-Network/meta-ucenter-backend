  "scripts": {
    "prebuild": "rimraf dist",
    "build": "nest build",
    "format": "prettier --write \"src/**/*.ts\" \"test/**/*.ts\"",
    "start": "nest start",
    "start:dev": "nest start --watch",
    "start:debug": "nest start --debug --watch",
    "start:prod": "nest build && nest start",
    "lint": "eslint \"{src,apps,libs,test}/**/*.ts\" --fix",
    "typeorm": "ts-node -r tsconfig-paths/register ./node_modules/typeorm/cli.js --config ./ormconfig.js",
    "keygen": "openssl genrsa -out JWT_PRIVATE_KEY.pemm 1024 && openssl rsa -in JWT_PRIVATE_KEY.pemm -pubout > JWT_PUBLIC_KEY.pubb",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:cov": "jest --coverage",
    "test:debug": "node --inspect-brk -r tsconfig-paths/register -r ts-node/register node_modules/.bin/jest --runInBand",
    "test:e2e": "jest --config ./test/jest-e2e.json",
    "prepare": "husky install"
  },
  "dependencies": {
    "@metaio/microservice-model": "^1.0.0",
    "@nestjs/axios": "^0.0.1",
    "@nestjs/common": "^8.0.0",
    "@nestjs/config": "^1.0.0",
