    "nestjs-general-interceptor": "^1.0.4",
    "nestjs-real-ip": "^2.0.0",
    "otplib": "^12.0.1",
    "passport": "^0.5.2",
    "passport-jwt": "^4.0.0",
    "passport-magic-login": "^1.0.9",
    "randomstring": "^1.2.2",
    "reflect-metadata": "^0.1.13",
    "rimraf": "^3.0.2",
    "rxjs": "^7.5.5",
    "swagger-ui-express": "^4.3.0",
    "typeorm": "^0.2.45"
