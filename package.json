  "devDependencies": {
    "@commitlint/cli": "^16.2.3",
    "@commitlint/config-conventional": "^16.2.1",
    "@nestjs/cli": "^8.2.3",
    "@nestjs/schematics": "^8.0.8",
    "@nestjs/testing": "^8.4.1",
    "@types/cache-manager": "^3.4.3",
    "@types/cookie-parser": "^1.4.2",
    "@types/express": "^4.17.13",
    "@types/jest": "^27.4.1",
    "@types/node": "^17.0.21",
